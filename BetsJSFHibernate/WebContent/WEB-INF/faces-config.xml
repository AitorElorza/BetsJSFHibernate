<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
	<managed-bean>
		<managed-bean-name>bL</managed-bean-name>
		<managed-bean-class>Facade.BusinesLogic</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>CreateQuestions?faces-redirect=true</from-outcome>
			<to-view-id>/CreateQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>QueryQuestions?faces-redirect=true</from-outcome>
			<to-view-id>/QueryQuestions.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>CreateQuestions.xhtml</display-name>
		<from-view-id>/CreateQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Main?faces-redirect=true</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>QueryQuestions.xhtml</display-name>
		<from-view-id>/QueryQuestions.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Main?faces-redirect=true</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>Main.xhtml</display-name>
		<from-view-id>/Main.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ShowEvents?faces-redirect=true</from-outcome>
			<to-view-id>/ShowEvents.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>ShowEvents.xhtml</display-name>
		<from-view-id>/ShowEvents.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>Main?faces-redirect=true</from-outcome>
			<to-view-id>/Main.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
